{% extends 'base.html.twig' %}


{% block body %}
    <!-- Start All Pages -->
    <div class="all-page-title page-breadcrumb">
        <div class="container text-center">
            <div class="row">
                <div class="col-lg-12">
                    <h1>My Subscription</h1>
                </div>
            </div>
        </div>
    </div>
    <!-- End All Pages -->

    <!-- Start blog details -->
    <div class="blog-box">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="heading-title text-center">
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-xl-8 col-lg-8 col-12">
                    <div class="blog-inner-details-page">
                        <div class="blog-inner-box">

                            <div class="inner-blog-detail details-page">
                                <blockquote>
                                    <h3>Price :</h3> <h3>{{ sub.prix }} DT</h3><br>
                                    <h3>Start Date :</h3> <h3>{{ sub.dateDebut|date('Y m d') }} </h3><br>
                                    <h3>End Date  :</h3> <h3>{{ sub.dateFin|date('Y m d') }} </h3><br>
                                </blockquote>

                            </div>
                        </div>


                    </div>
                </div>

                {# Right #}



            </div>
        </div>
    </div>

    <!-- End details -->
{% endblock %}
